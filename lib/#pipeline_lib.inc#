#! /bin/bash

# Function to 

get_pipeline_default_parameters()
{
    INFILE=$1 
    while read line
    do
	key=$(echo "$line" | awk -F"=" '{print $1}');
	if [[ ${key} =~ "#" || -z ${key} ]]
	    then
	    continue
	    else
	    value=$(echo "$line" | awk -F"=" '{print $2;}')
	    PARAMETERS_TABLE[${key}]=${value}
	    #echo -e "$key\t$value"
	    #echo -e "$key\t${PARAMETERS_TABLE[${key}]}"

	fi    
    done < $INFILE
}
